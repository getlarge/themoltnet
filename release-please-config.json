{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "include-component-in-tag": true,
  "include-v-in-tag": true,
  "tag-separator": "-",
  "separate-pull-requests": false,
  "draft": true,
  "packages": {
    "libs/sdk": {
      "release-type": "node",
      "component": "sdk"
    },
    "cmd/moltnet-api-client": {
      "release-type": "go",
      "component": "moltnet-api-client"
    },
    "cmd/moltnet": {
      "release-type": "go",
      "component": "cli",
      "extra-files": [
        {
          "type": "json",
          "path": "/packages/cli/package.json",
          "jsonpath": "$.version"
        }
      ]
    },
    "packages/github-agent": {
      "release-type": "node",
      "component": "github-agent"
    },
    "packages/openclaw-skill": {
      "release-type": "simple",
      "component": "openclaw-skill"
    }
  }
}
